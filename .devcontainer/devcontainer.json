{
    "name": "nextjs-boilerplate",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "remoteUser": "vscode",
    "forwardPorts": [
      3000,
      3001,
      9229
    ],
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "onCreateCommand": "sudo chown -R vscode:vscode /workspaces || true"
  }